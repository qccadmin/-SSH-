<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
     <package name="houselist" extends="base">
        <action name="houselist_*" class="action.HouseListAction" method="{1}">
           <result name="ok">/pro_zu.jsp</result>
           <result name="ajax">/house_list_ajax.jsp</result>
           <result name="map">/map.jsp</result>
           
        </action>
        <action name="medium_*" class="action.MediumAction" method="{1}">
        <result name="myhouse">/medium_house.jsp</result>
        <result name="houseajax">/hajax.jsp</result>
        </action>
     </package>
</struts>
